{
  "name": "rab-q",
  "version": "2.1.0",
  "description": "A tiny (opinionated) wrapper over amqplib for RabbitMQ publish/subscribe pattern",
  "license": "CECILL-B",
  "repository": "radiofrance/rab-q",
  "author": {
    "name": "Radio France",
    "email": "opensource@radiofrance.com",
